@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix wgs84: <http://www.w3.org/2003/01/geo/wgs84_pos#>.

<#geowgs84>
	a sh:NodeShape ;
	sh:targetSubjectsOf wgs84:long ;

	sh:property [
		sh:path wgs84:long ;
		sh:minExclusive "-180" ;
		sh:maxExclusive "180"
	] ;
	sh:property [
		sh:path wgs84:lat ;
		sh:minExclusive "-90" ;
		sh:maxExclusive "90"
	] .

